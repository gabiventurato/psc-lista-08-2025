package Lista8;

import java.util.Scanner;

public class Nnumeros {
    public static void main(String[] args) {
    Numeros();    
    }
public static void Numeros(){
    Scanner scanner = new Scanner(System.in);
    int num;
    System.out.println("Digite um n√∫mero");
    num = scanner.nextInt();
            for (int i = 1; i <= num; i++) {
        
            for (int j = 1; j <= i; j++) {
                System.out.print(i + " ");
}
System.out.println("");
}
}
}
